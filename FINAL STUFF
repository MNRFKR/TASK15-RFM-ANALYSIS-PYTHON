      "source": [
        "def segment_customer(score):\n",
        "    if score >= 9:\n",
        "        return \"Champion\"\n",
        "    elif score >= 7:\n",
        "        return \"Loyal\"\n",
        "    elif score >= 5:\n",
        "        return \"Potential Loyalist\"\n",
        "    else:\n",
        "        return \"At Risk\"\n",
        "\n",
        "df[\"Segment\"] = df[\"RFM_Score\"].apply(segment_customer)\n",
        "\n",
        "print(df[[\"Recency\",\"Frequency\",\"Monetary\",\"RFM_Score\",\"Segment\"]].head(10))"
      ],
