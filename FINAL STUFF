      "cell_type": "code",
      "source": [
        "df[\"R_score\"] = pd.qcut(df[\"Recency\"], 4, labels=[4,3,2,1])\n",
        "df[\"F_score\"] = pd.qcut(df[\"Frequency\"], 4, labels=[1,2,3,4])\n",
        "df[\"M_score\"] = pd.cut(df[\"Monetary\"], bins=3, labels=[1,2,3])\n",
        "df[\"R_score\"] = pd.qcut(df[\"Recency\"], 4, labels=[4,3,2,1])\n",
        "df[\"F_score\"] = pd.qcut(df[\"Frequency\"], 4, labels=[1,2,3,4])\n",
        "df[\"RFM_Score\"] = df[\"R_score\"].astype(int) + df[\"F_score\"].astype(int) + df[\"M_score\"].astype(int)\n",
        "\n",
        "print(df[[\"Recency\",\"Frequency\",\"Monetary\",\"R_score\",\"F_score\",\"M_score\",\"RFM_Score\"]].head(10))\n"
